sub F_LOCK {

	$qhit=0;
	for($qp=0;$qp<1;$qp++){
		if (-d $lockfile) {
			$qhit=1;
			sleep(1);
			last;
 		}else{
			mkdir($lockfile, 0755);
		}
	}

	if($qhit){
     	&UNLOCK_FILE();
	 	&ERR("File lock error!<BR>파일 갱신중입니다. 잠시만 기다려 주세요.");
	}

}

